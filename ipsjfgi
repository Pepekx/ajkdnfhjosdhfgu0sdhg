	local function createWeld(x,y)
		weld = Instance.new("Weld", x)
		weld.Part0 = x
		weld.Part1 = y

		local CJ = CFrame.new(x.Position)

		weld.C0 = x.CFrame:inverse() * CJ
		weld.C1 = y.CFrame:inverse() * CJ
	end
	
	for _,v in pairs(Model:GetDescendants()) do
		if v:IsA("BasePart") and v ~= Att then
			spawn(function()
				createWeld(Att, v)
				v.Material = Enum.Material.SmoothPlastic
				wait(1)
				v.Anchored = false
			end)
		end
	end
	
	char.UpperTorso.Anchored = true
	Att.Orientation = Vector3.new(0, 180, 0)
	
	wait(1)
	
	Att.CFrame = char.UpperTorso.CFrame
	
	wait(.5)
	
	createWeld(Att, char.UpperTorso)
	
	wait(1)
	
	char.UpperTorso.Anchored = false
	Att.Anchored = false
	
end

Initialize.MouseButton1Click:Connect(function()
	if isInitialized == false then
		initialize(plr, plr.Character)
	end
end)

local UserInputService = game:GetService("UserInputService")

local gui = Main

local dragging
local dragInput
local dragStart
local startPos

local function update(input)
	local delta = input.Position - dragStart
	gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

gui.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = gui.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

gui.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)

local green = Color3.fromRGB(0, 255, 183)
local red = Color3.fromRGB(255, 79, 79)

UTOGGLE.OptionTitle.MouseButton1Click:Connect(function()
	plr.Character.Pants.PantsTemplate = "rbxassetid://8760911089"
	plr.Character.Shirt.ShirtTemplate = "rbxassetid://8760910831"
	UTOGGLE.Toggle.BackgroundColor3 = green
	CTOGGLE.Toggle.BackgroundColor3 = red
	
	UndieF.Transparency = 0
	Middle.Transparency = 0
end)
UTOGGLE.OptionTitle.MouseButton2Click:Connect(function()
	UTOGGLE.Toggle.BackgroundColor3 = red

	UndieF.Transparency = 1
	Middle.Transparency = 1
end)


UYELLOW.OptionTitle.MouseButton1Click:Connect(function()
	UYELLOW.Toggle.BackgroundColor3 = green
	
	UndieF.Color = Color3.fromRGB(255, 255, 233)
	Yellow_UndieF.Enabled = true
end)
UYELLOW.OptionTitle.MouseButton2Click:Connect(function()
	UYELLOW.Toggle.BackgroundColor3 = red

	UndieF.Color = Color3.fromRGB(255, 255, 255)
	Yellow_UndieF.Enabled = false
end)


UBROWN.OptionTitle.MouseButton1Click:Connect(function()
	UBROWN.Toggle.BackgroundColor3 = green
	
	Brown.Transparency = 0
end)
UBROWN.OptionTitle.MouseButton2Click:Connect(function()
	UBROWN.Toggle.BackgroundColor3 = red

	Brown.Transparency = 1
end)

currentlyE = false
ptoggled = false

PYELLOW.OptionTitle.MouseButton1Click:Connect(function()
	if ptoggled == true then
		PYELLOW.Toggle.BackgroundColor3 = green
		if currentlyE == true then
			TopEmitter_Yellow1.Enabled = true
			TopEmitter_Yellow.Enabled = true
			LowEmitter_Yellow.Enabled = false
			LowEmitter_Yellow1.Enabled = false
		elseif currentlyE == false then
			TopEmitter_Yellow1.Enabled = false
			TopEmitter_Yellow.Enabled = false
			LowEmitter_Yellow.Enabled = true
			LowEmitter_Yellow1.Enabled = true
		end
	end
end)
PYELLOW.OptionTitle.MouseButton2Click:Connect(function()
	PYELLOW.Toggle.BackgroundColor3 = red
	TopEmitter_Yellow1.Enabled = false
	TopEmitter_Yellow.Enabled = false
	LowEmitter_Yellow.Enabled = false
	LowEmitter_Yellow1.Enabled = false
end)

ETOGGLE.OptionTitle.MouseButton1Click:Connect(function()
	if ptoggled == true then
		ETOGGLE.Toggle.BackgroundColor3 = green
		
		currentlyE = true
		Top.Transparency = 0
		Low.Transparency = 0
	end
end)
ETOGGLE.OptionTitle.MouseButton2Click:Connect(function()
	if ptoggled == true then
		ETOGGLE.Toggle.BackgroundColor3 = red

		currentlyE = false
		Top.Transparency = 1
		Low.Transparency = 1
	end
end)

PTOGGLE.OptionTitle.MouseButton1Click:Connect(function()
	
	if ptoggled ~= true then
		CTOGGLE.Toggle.BackgroundColor3 = red
		plr.Character.Shirt.ShirtTemplate = "rbxassetid://8770741624"
		plr.Character.Pants.PantsTemplate = "rbxassetid://8770741790"
		PTOGGLE.Toggle.BackgroundColor3 = green
		ptoggled = true
		
		Ball1.Transparency = 0
		Ball2.Transparency = 0
		Shaft.Transparency = 0
		FLACC.Transparency = 0
	end
end)

PTOGGLE.OptionTitle.MouseButton2Click:Connect(function()
	if ptoggled == true then
		PTOGGLE.Toggle.BackgroundColor3 = red
		ETOGGLE.Toggle.BackgroundColor3 = red
		ptoggled = false

		Ball1.Transparency = 1
		Ball2.Transparency = 1
		Shaft.Transparency = 1
		FLACC.Transparency = 1
		if currentlyE == true then
			Top.Transparency = 1
			Low.Transparency = 1
			currentlyE = false
		end
	end
end)

UserInputService.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.J then
		bool = not bool
		if bool then
			UI.Enabled = false
		else
			UI.Enabled = true
		end
	end
end)

CTOGGLE.OptionTitle.MouseButton1Click:Connect(function()
	currentlyE = false
	ptoggled = false
	
	
	Ball1.Transparency = 1
	Ball2.Transparency = 1
	Shaft.Transparency = 1
	FLACC.Transparency = 1
	Top.Transparency = 1
	Low.Transparency = 1
	UndieF.Transparency = 1
	Middle.Transparency = 1
	Brown.Transparency = 1
	
	TopEmitter_Yellow1.Enabled = false
	TopEmitter_Yellow.Enabled = false
	LowEmitter_Yellow.Enabled = false
	LowEmitter_Yellow1.Enabled = false
	Yellow_UndieF.Enabled = false
	
	CTOGGLE.Toggle.BackgroundColor3 = green
	PTOGGLE.Toggle.BackgroundColor3 = red
	ETOGGLE.Toggle.BackgroundColor3 = red
	PYELLOW.Toggle.BackgroundColor3 = red
	UBROWN.Toggle.BackgroundColor3 = red
	UYELLOW.Toggle.BackgroundColor3 = red
	UTOGGLE.Toggle.BackgroundColor3 = red
	
	plr.Character.Shirt.ShirtTemplate = "rbxassetid://8738842253"
	plr.Character.Pants.PantsTemplate = "rbxassetid://8738846115"
end)
