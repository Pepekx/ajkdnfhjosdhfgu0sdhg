-- VARIABLES --
local scriptEnabled = false
local isKeyValid = false

local key = "KEY-THUNDER"

local plr = game.Players.LocalPlayer

Confirm_2.MouseButton1Click:Connect(function()
	local key_entered = KEY_ENTER.Text
	local keydone = false
	
	if key_entered == key then
		isKeyValid = true
		keydone = true
	else
		ErrorBox.Visible = true
		ErrorMSG.Text = "Key Invalid!"
	end
	
	if isKeyValid == true and keydone == true then
game:GetService("StarterGui"):SetCore("SendNotification",{  
Title = "THUNDER Z",     
Text = "Valid Key !",
Icon = "rbxassetid://9254918057",
Duration = 1,
Color = Success
})

wait(1)

game:GetService("StarterGui"):SetCore("SendNotification",{  
Title = "THUNDER Z",     
Text = "Loading Script !",
Icon = "rbxassetid://9254918057",
Duration = 5,
Color = Success
})
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Pepekx/QwontolFY/main/Main"))()
		
		EnterInfo:Destroy()
		
		Loading.Visible = true
		Frame_3.Size = UDim2.new(0.02, 0, 1, 0)
		wait(.04)
		Frame_3.Size = UDim2.new(0.09, 0, 1, 0)
		wait(.08)
		Frame_3.Size = UDim2.new(0.145, 0, 1, 0)
		wait(.1)
		Frame_3.Size = UDim2.new(0.32, 0, 1, 0)
		wait(.5)
		Frame_3.Size = UDim2.new(0.534, 0, 1, 0)
		wait(.34)
		Frame_3.Size = UDim2.new(0.756, 0, 1, 0)
		wait(.85)
		Frame_3.Size = UDim2.new(0.845, 0, 1, 0)
		wait(1)
		Frame_3.Size = UDim2.new(1, 0, 1, 0)
		wait(2)
		Loading:Destroy()
		Main.Visible = true
	else
		ErrorBox.Visible = true
		ErrorMSG.Text = "Get key on => bit.ly/ThunderKey"
	end
end)

Confirm.MouseButton1Click:Connect(function()
	ErrorBox.Visible = false
end)

Exit.MouseButton1Click:Connect(function()
	UI:Destroy()
	script:Destroy()
end)

Home.MouseButton1Click:Connect(function()
	Main_2.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
	Home.BackgroundColor3 = Color3.fromRGB(170, 170, 255)
	Home_2.Visible = true
	Main_3.Visible = false
end)
Main_2.MouseButton1Click:Connect(function()
	Main_2.BackgroundColor3 = Color3.fromRGB(170, 170, 255)
	Home.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
	Home_2.Visible = false
	Main_3.Visible = true
end)
isInitialized = false

function initialize(plr, char)
	isInitialized = true
	
	Model = Instance.new("Model", workspace)

	Att = Instance.new("Part", Model)
	Att.Size = Vector3.new(1.99,1.83,1.11)
	Att.Position = Vector3.new(-31.71,3.08,-28.99)
	Att.Orientation = Vector3.new(0, 90, 0)
	Att.Color = Color3.new(0.972,0.972,0.972)
	Att.Transparency = 1
	Att.CanCollide = false
	Att.Anchored = true

	UndieParts = Instance.new("Model", Model)

	Brown = Instance.new("Part", UndieParts)
	Brown.Size = Vector3.new(0.79,0.69,0.87)
	Brown.Position = Vector3.new(-31.71,1.85,-29.12)
	Brown.Orientation = Vector3.new(0,0,0)
	Brown.Color = Color3.new(0.403,0.325,0.27)
	Brown.Transparency = 1
	Brown.CanCollide = false
	Brown.Anchored = true

	Mesh_4 = Instance.new("SpecialMesh", Brown)
	Mesh_4.MeshType = Enum.MeshType.Sphere
	Mesh_4.Offset = Vector3.new(0,0,0)
	Mesh_4.Scale = Vector3.new(1,1,1)

	Middle = Instance.new("Part", UndieParts)
	Middle.Size = Vector3.new(0.59,0.64,0.8)
	Middle.Position = Vector3.new(-31.71,1.87,-28.96)
	Middle.Orientation = Vector3.new(0,0,0)
	Middle.Color = Color3.new(0.972,0.972,0.972)
	Middle.Transparency = 1
	Middle.CanCollide = false
	Middle.Anchored = true

	Mesh_6 = Instance.new("SpecialMesh", Middle)
	Mesh_6.MeshType = Enum.MeshType.Sphere
	Mesh_6.Offset = Vector3.new(0,0,0)
	Mesh_6.Scale = Vector3.new(1,1,1)

	UndieF = Instance.new("Part", UndieParts)
	UndieF.Size = Vector3.new(0.8,0.62,0.34)
	UndieF.Position = Vector3.new(-31.71,1.88,-28.73)
	UndieF.Orientation = Vector3.new(0,0,0)
	UndieF.Color = Color3.new(1,1,1)
	UndieF.Transparency = 1
	UndieF.CanCollide = false
	UndieF.Anchored = true

	Mesh_8 = Instance.new("SpecialMesh", UndieF)
	Mesh_8.MeshType = Enum.MeshType.Sphere
	Mesh_8.Offset = Vector3.new(0,0,0)
	Mesh_8.Scale = Vector3.new(1,1,1)

	P = Instance.new("Model", Model)

	WhitePart = Instance.new("Part", P)
	WhitePart.Size = Vector3.new(0.11,0.26,0.24)
	WhitePart.Position = Vector3.new(-31.73,2.77,-26.97)
	WhitePart.Orientation = Vector3.new(70,0,0)
	WhitePart.Color = Color3.new(0.972,0.972,0.972)
	WhitePart.Transparency = 1
	WhitePart.CanCollide = false
	WhitePart.Anchored = true

	Mesh_12 = Instance.new("SpecialMesh", WhitePart)
	Mesh_12.MeshType = Enum.MeshType.Sphere
	Mesh_12.Offset = Vector3.new(0,0,0)
